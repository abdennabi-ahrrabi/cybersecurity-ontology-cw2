<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ulster/ontologies/2025/cybersecurity#"
     xml:base="http://www.semanticweb.org/ulster/ontologies/2025/cybersecurity"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:cyber="http://www.semanticweb.org/ulster/ontologies/2025/cybersecurity#">

    <!-- ============================================== -->
    <!-- ONTOLOGY METADATA                              -->
    <!-- ============================================== -->

    <owl:Ontology rdf:about="http://www.semanticweb.org/ulster/ontologies/2025/cybersecurity">
        <owl:versionIRI rdf:resource="http://www.semanticweb.org/ulster/ontologies/2025/cybersecurity/1.0"/>
        <dc:creator>Abdennabi Ahrrabi - Ulster University MSc</dc:creator>
        <dc:description>A focused OWL ontology for modeling cybersecurity incident response scenarios.
This ontology supports:
- Threat and vulnerability management
- Asset protection and risk assessment
- Security incident lifecycle tracking
- Compliance mapping to security frameworks

Domain Focus: Enterprise Network Security Incident Response</dc:description>
        <dc:title>Cybersecurity Incident Response Ontology</dc:title>
    </owl:Ontology>

    <!-- ============================================== -->
    <!-- ANNOTATION PROPERTIES                          -->
    <!-- ============================================== -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>

    <!-- ============================================== -->
    <!-- OBJECT PROPERTIES (12 properties)              -->
    <!-- ============================================== -->

    <!-- Property: targets -->
    <!-- PROPERTY CHAIN: If a threat exploits a vulnerability that affects an asset,
         then the threat targets that asset (targets ⊑ exploits ∘ affects) -->
    <owl:ObjectProperty rdf:about="#targets">
        <owl:inverseOf rdf:resource="#isTargetedBy"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="#exploits"/>
            <rdf:Description rdf:about="#affects"/>
        </owl:propertyChainAxiom>
        <rdfs:domain rdf:resource="#Threat"/>
        <rdfs:range rdf:resource="#Asset"/>
        <rdfs:comment>A threat targets one or more assets. Can be inferred via property chain: exploits ∘ affects → targets</rdfs:comment>
        <rdfs:label>targets</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isTargetedBy">
        <rdfs:domain rdf:resource="#Asset"/>
        <rdfs:range rdf:resource="#Threat"/>
        <rdfs:label>isTargetedBy</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: exploits -->
    <owl:ObjectProperty rdf:about="#exploits">
        <owl:inverseOf rdf:resource="#isExploitedBy"/>
        <rdfs:domain rdf:resource="#Threat"/>
        <rdfs:range rdf:resource="#Vulnerability"/>
        <rdfs:comment>A threat exploits vulnerabilities to compromise assets</rdfs:comment>
        <rdfs:label>exploits</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isExploitedBy">
        <rdfs:domain rdf:resource="#Vulnerability"/>
        <rdfs:range rdf:resource="#Threat"/>
        <rdfs:label>isExploitedBy</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: affects -->
    <owl:ObjectProperty rdf:about="#affects">
        <owl:inverseOf rdf:resource="#isAffectedBy"/>
        <rdfs:domain rdf:resource="#Vulnerability"/>
        <rdfs:range rdf:resource="#Asset"/>
        <rdfs:comment>A vulnerability affects specific assets</rdfs:comment>
        <rdfs:label>affects</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isAffectedBy">
        <rdfs:domain rdf:resource="#Asset"/>
        <rdfs:range rdf:resource="#Vulnerability"/>
        <rdfs:label>isAffectedBy</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: mitigates -->
    <owl:ObjectProperty rdf:about="#mitigates">
        <owl:inverseOf rdf:resource="#isMitigatedBy"/>
        <rdfs:domain rdf:resource="#SecurityControl"/>
        <rdfs:comment>A security control mitigates threats or vulnerabilities</rdfs:comment>
        <rdfs:label>mitigates</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isMitigatedBy">
        <rdfs:range rdf:resource="#SecurityControl"/>
        <rdfs:label>isMitigatedBy</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: protects -->
    <owl:ObjectProperty rdf:about="#protects">
        <owl:inverseOf rdf:resource="#isProtectedBy"/>
        <rdfs:domain rdf:resource="#SecurityControl"/>
        <rdfs:range rdf:resource="#Asset"/>
        <rdfs:comment>A security control protects assets</rdfs:comment>
        <rdfs:label>protects</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isProtectedBy">
        <rdfs:domain rdf:resource="#Asset"/>
        <rdfs:range rdf:resource="#SecurityControl"/>
        <rdfs:label>isProtectedBy</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: triggeredBy -->
    <owl:ObjectProperty rdf:about="#triggeredBy">
        <owl:inverseOf rdf:resource="#triggers"/>
        <rdfs:domain rdf:resource="#SecurityIncident"/>
        <rdfs:range rdf:resource="#Threat"/>
        <rdfs:comment>An incident is triggered by a threat</rdfs:comment>
        <rdfs:label>triggeredBy</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#triggers">
        <rdfs:domain rdf:resource="#Threat"/>
        <rdfs:range rdf:resource="#SecurityIncident"/>
        <rdfs:label>triggers</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: detectedBy -->
    <owl:ObjectProperty rdf:about="#detectedBy">
        <owl:inverseOf rdf:resource="#detects"/>
        <rdfs:domain rdf:resource="#SecurityIncident"/>
        <rdfs:range rdf:resource="#SecurityControl"/>
        <rdfs:comment>An incident is detected by security controls</rdfs:comment>
        <rdfs:label>detectedBy</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#detects">
        <rdfs:domain rdf:resource="#SecurityControl"/>
        <rdfs:range rdf:resource="#SecurityIncident"/>
        <rdfs:label>detects</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: handledBy -->
    <owl:ObjectProperty rdf:about="#handledBy">
        <owl:inverseOf rdf:resource="#handles"/>
        <rdfs:domain rdf:resource="#SecurityIncident"/>
        <rdfs:range rdf:resource="#IncidentResponse"/>
        <rdfs:comment>An incident is handled by a response procedure</rdfs:comment>
        <rdfs:label>handledBy</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#handles">
        <rdfs:domain rdf:resource="#IncidentResponse"/>
        <rdfs:range rdf:resource="#SecurityIncident"/>
        <rdfs:label>handles</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: performedBy -->
    <owl:ObjectProperty rdf:about="#performedBy">
        <owl:inverseOf rdf:resource="#performs"/>
        <rdfs:domain rdf:resource="#IncidentResponse"/>
        <rdfs:range rdf:resource="#SecurityTeam"/>
        <rdfs:comment>A response action is performed by a security team</rdfs:comment>
        <rdfs:label>performedBy</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#performs">
        <rdfs:domain rdf:resource="#SecurityTeam"/>
        <rdfs:range rdf:resource="#IncidentResponse"/>
        <rdfs:label>performs</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: impactsAsset -->
    <owl:ObjectProperty rdf:about="#impactsAsset">
        <rdfs:domain rdf:resource="#SecurityIncident"/>
        <rdfs:range rdf:resource="#Asset"/>
        <rdfs:comment>An incident impacts specific assets</rdfs:comment>
        <rdfs:label>impactsAsset</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: hasOwner -->
    <owl:ObjectProperty rdf:about="#hasOwner">
        <rdfs:domain rdf:resource="#Asset"/>
        <rdfs:range rdf:resource="#Organization"/>
        <rdfs:comment>An asset has an owner organization</rdfs:comment>
        <rdfs:label>hasOwner</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: compliesWith -->
    <owl:ObjectProperty rdf:about="#compliesWith">
        <rdfs:domain rdf:resource="#SecurityControl"/>
        <rdfs:range rdf:resource="#ComplianceFramework"/>
        <rdfs:comment>A security control complies with regulatory frameworks</rdfs:comment>
        <rdfs:label>compliesWith</rdfs:label>
    </owl:ObjectProperty>

    <!-- Property: partOf (transitive) -->
    <owl:ObjectProperty rdf:about="#partOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <owl:inverseOf rdf:resource="#hasPart"/>
        <rdfs:comment>Transitive containment relationship for assets</rdfs:comment>
        <rdfs:label>partOf</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasPart">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>

    <!-- ============================================== -->
    <!-- DATA PROPERTIES (10 properties)               -->
    <!-- ============================================== -->

    <owl:DatatypeProperty rdf:about="#hasName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name identifier for any entity</rdfs:comment>
        <rdfs:label>hasName</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasCVSSScore">
        <rdfs:domain rdf:resource="#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>CVSS score between 0.0 and 10.0</rdfs:comment>
        <rdfs:label>hasCVSSScore</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasCVEIdentifier">
        <rdfs:domain rdf:resource="#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>CVE identifier (e.g., CVE-2024-1234)</rdfs:comment>
        <rdfs:label>hasCVEIdentifier</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasSeverityLevel">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Severity: Critical, High, Medium, Low, Info</rdfs:comment>
        <rdfs:label>hasSeverityLevel</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasTimestamp">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp for events and incidents</rdfs:comment>
        <rdfs:label>hasTimestamp</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasIPAddress">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#NetworkAsset"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>IP address of network asset. Functional: each asset has exactly one primary IP.</rdfs:comment>
        <rdfs:label>hasIPAddress</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasStatus">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Status: Active, Resolved, Pending, Contained</rdfs:comment>
        <rdfs:label>hasStatus</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Detailed description text</rdfs:comment>
        <rdfs:label>hasDescription</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hasCriticality">
        <rdfs:domain rdf:resource="#Asset"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Asset criticality: Mission-Critical, High, Medium, Low</rdfs:comment>
        <rdfs:label>hasCriticality</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#isPatched">
        <rdfs:domain rdf:resource="#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Whether the vulnerability has been patched</rdfs:comment>
        <rdfs:label>isPatched</rdfs:label>
    </owl:DatatypeProperty>

    <!-- ============================================== -->
    <!-- CLASSES (40 classes in clear hierarchy)       -->
    <!-- ============================================== -->

    <!-- ==================== -->
    <!-- TOP-LEVEL CLASSES    -->
    <!-- ==================== -->

    <!-- Asset Class Hierarchy -->
    <owl:Class rdf:about="#Asset">
        <rdfs:comment>Any resource of value that needs protection. Assets can be physical, digital, or human resources.</rdfs:comment>
        <rdfs:label>Asset</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#PhysicalAsset">
        <rdfs:subClassOf rdf:resource="#Asset"/>
        <rdfs:comment>Tangible physical resources including hardware and facilities</rdfs:comment>
        <rdfs:label>PhysicalAsset</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#DigitalAsset">
        <rdfs:subClassOf rdf:resource="#Asset"/>
        <rdfs:comment>Intangible digital resources including data and software</rdfs:comment>
        <rdfs:label>DigitalAsset</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#NetworkAsset">
        <rdfs:subClassOf rdf:resource="#PhysicalAsset"/>
        <rdfs:comment>Network infrastructure components with IP addresses</rdfs:comment>
        <rdfs:label>NetworkAsset</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Server">
        <rdfs:subClassOf rdf:resource="#NetworkAsset"/>
        <rdfs:comment>Computing server providing network services</rdfs:comment>
        <rdfs:label>Server</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Workstation">
        <rdfs:subClassOf rdf:resource="#NetworkAsset"/>
        <rdfs:comment>End-user computing device</rdfs:comment>
        <rdfs:label>Workstation</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#NetworkDevice">
        <rdfs:subClassOf rdf:resource="#NetworkAsset"/>
        <rdfs:comment>Network infrastructure devices (routers, switches, firewalls)</rdfs:comment>
        <rdfs:label>NetworkDevice</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Database">
        <rdfs:subClassOf rdf:resource="#DigitalAsset"/>
        <rdfs:comment>Structured data storage system</rdfs:comment>
        <rdfs:label>Database</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Application">
        <rdfs:subClassOf rdf:resource="#DigitalAsset"/>
        <rdfs:comment>Software application or service</rdfs:comment>
        <rdfs:label>Application</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#SensitiveData">
        <rdfs:subClassOf rdf:resource="#DigitalAsset"/>
        <rdfs:comment>Data requiring special protection (PII, PHI, financial)</rdfs:comment>
        <rdfs:label>SensitiveData</rdfs:label>
    </owl:Class>

    <!-- Threat Class Hierarchy -->
    <owl:Class rdf:about="#Threat">
        <rdfs:comment>Potential cause of an unwanted incident that may harm assets</rdfs:comment>
        <rdfs:label>Threat</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Malware">
        <rdfs:subClassOf rdf:resource="#Threat"/>
        <rdfs:comment>Malicious software designed to damage or exploit systems</rdfs:comment>
        <rdfs:label>Malware</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Ransomware">
        <rdfs:subClassOf rdf:resource="#Malware"/>
        <rdfs:comment>Malware that encrypts data and demands ransom payment</rdfs:comment>
        <rdfs:label>Ransomware</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Trojan">
        <rdfs:subClassOf rdf:resource="#Malware"/>
        <rdfs:comment>Malware disguised as legitimate software</rdfs:comment>
        <rdfs:label>Trojan</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#NetworkAttack">
        <rdfs:subClassOf rdf:resource="#Threat"/>
        <rdfs:comment>Attack targeting network infrastructure or protocols</rdfs:comment>
        <rdfs:label>NetworkAttack</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#PhishingAttack">
        <rdfs:subClassOf rdf:resource="#Threat"/>
        <rdfs:comment>Social engineering attack using deceptive communications</rdfs:comment>
        <rdfs:label>PhishingAttack</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#InsiderThreat">
        <rdfs:subClassOf rdf:resource="#Threat"/>
        <rdfs:comment>Threat originating from authorized users within the organization</rdfs:comment>
        <rdfs:label>InsiderThreat</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#ThreatActor">
        <rdfs:comment>Entity responsible for initiating threats</rdfs:comment>
        <rdfs:label>ThreatActor</rdfs:label>
    </owl:Class>

    <!-- Vulnerability Class Hierarchy -->
    <owl:Class rdf:about="#Vulnerability">
        <rdfs:comment>Weakness in a system that can be exploited by threats</rdfs:comment>
        <rdfs:label>Vulnerability</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#SoftwareVulnerability">
        <rdfs:subClassOf rdf:resource="#Vulnerability"/>
        <rdfs:comment>Vulnerability in software code or configuration</rdfs:comment>
        <rdfs:label>SoftwareVulnerability</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#ConfigurationVulnerability">
        <rdfs:subClassOf rdf:resource="#Vulnerability"/>
        <rdfs:comment>Vulnerability due to misconfiguration</rdfs:comment>
        <rdfs:label>ConfigurationVulnerability</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#ZeroDayVulnerability">
        <rdfs:subClassOf rdf:resource="#SoftwareVulnerability"/>
        <rdfs:comment>Unpatched vulnerability unknown to the vendor</rdfs:comment>
        <rdfs:label>ZeroDayVulnerability</rdfs:label>
    </owl:Class>

    <!-- DEFINED CLASS: Actively Exploited Vulnerability -->
    <!-- A vulnerability that is exploited by at least one threat -->
    <owl:Class rdf:about="#ActivelyExploitedVulnerability">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Vulnerability"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isExploitedBy"/>
                        <owl:someValuesFrom rdf:resource="#Threat"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Vulnerability currently being exploited by at least one threat - automatically classified by reasoner</rdfs:comment>
        <rdfs:label>ActivelyExploitedVulnerability</rdfs:label>
    </owl:Class>

    <!-- Security Control Class Hierarchy -->
    <owl:Class rdf:about="#SecurityControl">
        <rdfs:comment>Safeguard or countermeasure to protect assets and mitigate risks</rdfs:comment>
        <rdfs:label>SecurityControl</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#PreventiveControl">
        <rdfs:subClassOf rdf:resource="#SecurityControl"/>
        <rdfs:comment>Controls that prevent security incidents from occurring</rdfs:comment>
        <rdfs:label>PreventiveControl</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#DetectiveControl">
        <rdfs:subClassOf rdf:resource="#SecurityControl"/>
        <rdfs:comment>Controls that detect security incidents when they occur</rdfs:comment>
        <rdfs:label>DetectiveControl</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#CorrectiveControl">
        <rdfs:subClassOf rdf:resource="#SecurityControl"/>
        <rdfs:comment>Controls that correct or restore systems after incidents</rdfs:comment>
        <rdfs:label>CorrectiveControl</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Firewall">
        <rdfs:subClassOf rdf:resource="#PreventiveControl"/>
        <rdfs:comment>Network security device filtering traffic</rdfs:comment>
        <rdfs:label>Firewall</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#IDS">
        <rdfs:subClassOf rdf:resource="#DetectiveControl"/>
        <rdfs:comment>Intrusion Detection System</rdfs:comment>
        <rdfs:label>IDS</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#SIEM">
        <rdfs:subClassOf rdf:resource="#DetectiveControl"/>
        <rdfs:comment>Security Information and Event Management system</rdfs:comment>
        <rdfs:label>SIEM</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Antivirus">
        <rdfs:subClassOf rdf:resource="#PreventiveControl"/>
        <rdfs:subClassOf rdf:resource="#DetectiveControl"/>
        <rdfs:comment>Software that detects and prevents malware (both preventive and detective)</rdfs:comment>
        <rdfs:label>Antivirus</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#BackupSystem">
        <rdfs:subClassOf rdf:resource="#CorrectiveControl"/>
        <rdfs:comment>System for data backup and recovery</rdfs:comment>
        <rdfs:label>BackupSystem</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Encryption">
        <rdfs:subClassOf rdf:resource="#PreventiveControl"/>
        <rdfs:comment>Cryptographic protection for data</rdfs:comment>
        <rdfs:label>Encryption</rdfs:label>
    </owl:Class>

    <!-- Incident and Response Classes -->
    <owl:Class rdf:about="#SecurityIncident">
        <rdfs:comment>A security event that requires response action</rdfs:comment>
        <rdfs:label>SecurityIncident</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#DataBreach">
        <rdfs:subClassOf rdf:resource="#SecurityIncident"/>
        <rdfs:comment>Incident involving unauthorized access to sensitive data</rdfs:comment>
        <rdfs:label>DataBreach</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#RansomwareIncident">
        <rdfs:subClassOf rdf:resource="#SecurityIncident"/>
        <rdfs:comment>Incident involving ransomware encryption of systems</rdfs:comment>
        <rdfs:label>RansomwareIncident</rdfs:label>
    </owl:Class>

    <!-- DEFINED CLASS: Detected Incident -->
    <!-- An incident that has been detected by at least one security control -->
    <owl:Class rdf:about="#DetectedIncident">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#SecurityIncident"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#detectedBy"/>
                        <owl:someValuesFrom rdf:resource="#SecurityControl"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Incident that has been detected by security controls - automatically classified by reasoner</rdfs:comment>
        <rdfs:label>DetectedIncident</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#IncidentResponse">
        <rdfs:comment>Organized approach to addressing security incidents</rdfs:comment>
        <rdfs:label>IncidentResponse</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Containment">
        <rdfs:subClassOf rdf:resource="#IncidentResponse"/>
        <rdfs:comment>Actions to limit the spread of an incident</rdfs:comment>
        <rdfs:label>Containment</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Eradication">
        <rdfs:subClassOf rdf:resource="#IncidentResponse"/>
        <rdfs:comment>Actions to remove the threat from the environment</rdfs:comment>
        <rdfs:label>Eradication</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Recovery">
        <rdfs:subClassOf rdf:resource="#IncidentResponse"/>
        <rdfs:comment>Actions to restore systems to normal operation</rdfs:comment>
        <rdfs:label>Recovery</rdfs:label>
    </owl:Class>

    <!-- Organization Classes -->
    <owl:Class rdf:about="#Organization">
        <rdfs:comment>Entity that owns and operates assets</rdfs:comment>
        <rdfs:label>Organization</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#SecurityTeam">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:comment>Team responsible for security operations</rdfs:comment>
        <rdfs:label>SecurityTeam</rdfs:label>
    </owl:Class>

    <!-- Compliance Framework -->
    <owl:Class rdf:about="#ComplianceFramework">
        <rdfs:comment>Regulatory or industry security standard</rdfs:comment>
        <rdfs:label>ComplianceFramework</rdfs:label>
    </owl:Class>

    <!-- ============================================== -->
    <!-- ADDITIONAL DEFINED CLASSES (Reasoner Inferred) -->
    <!-- ============================================== -->

    <!-- DEFINED CLASS: High Severity Vulnerability -->
    <!-- Vulnerability with severity level "Critical" or "High" -->
    <owl:Class rdf:about="#HighSeverityVulnerability">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Vulnerability"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#hasSeverityLevel"/>
                                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Critical</owl:hasValue>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="#hasSeverityLevel"/>
                                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">High</owl:hasValue>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Vulnerability with Critical or High severity - automatically classified by reasoner</rdfs:comment>
        <rdfs:label>HighSeverityVulnerability</rdfs:label>
    </owl:Class>

    <!-- DEFINED CLASS: Protected Asset -->
    <!-- Asset that is protected by at least one security control -->
    <owl:Class rdf:about="#ProtectedAsset">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Asset"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isProtectedBy"/>
                        <owl:someValuesFrom rdf:resource="#SecurityControl"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Asset protected by at least one security control - automatically classified by reasoner</rdfs:comment>
        <rdfs:label>ProtectedAsset</rdfs:label>
    </owl:Class>

    <!-- DEFINED CLASS: Vulnerable Asset -->
    <!-- Asset that is affected by at least one unpatched vulnerability -->
    <owl:Class rdf:about="#VulnerableAsset">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Asset"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isAffectedBy"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="#Vulnerability"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="#isPatched"/>
                                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</owl:hasValue>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Asset affected by at least one unpatched vulnerability - automatically classified by reasoner</rdfs:comment>
        <rdfs:label>VulnerableAsset</rdfs:label>
    </owl:Class>

    <!-- DEFINED CLASS: Mitigated Threat -->
    <!-- Threat that is mitigated by at least one security control -->
    <owl:Class rdf:about="#MitigatedThreat">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Threat"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#isMitigatedBy"/>
                        <owl:someValuesFrom rdf:resource="#SecurityControl"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Threat that has mitigation controls in place - automatically classified by reasoner</rdfs:comment>
        <rdfs:label>MitigatedThreat</rdfs:label>
    </owl:Class>

    <!-- DEFINED CLASS: HIPAA Compliant Control -->
    <!-- Security control that complies with HIPAA -->
    <owl:Class rdf:about="#HIPAACompliantControl">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#SecurityControl"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#compliesWith"/>
                        <owl:hasValue rdf:resource="#HIPAA"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Security control compliant with HIPAA regulations - automatically classified by reasoner</rdfs:comment>
        <rdfs:label>HIPAACompliantControl</rdfs:label>
    </owl:Class>

